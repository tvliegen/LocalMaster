<div class="row">
<div class="col-md-12"> 

 <div class="col-md-1"></div>

 <div class="col-md-11">

 
    <h3>Portal Administrator - All Applications</h3>
    </div>
 </div>
 
 
 <div class="row">
 <div class="col-md-12"> 
  <div class="col-md-1"></div>
 <div class="col-md-11">
<div class="col-md-12">
      	<%= link_to "New", "/portalmanager/appdefs/new", class: "btn btn-default" %>
</div>
</div>
 </div>
 </div>
 
 <div class="row">
  <div class="col-md-12"> 
  <div class="col-md-1"></div>
   <div class="col-md-11">
    <div class="col-md-10">
     <div class="table-responsive">
      <table class="table table-striped table-hover">
       <thead>
        <tr>
         <th>
          Application Name</th>
         <th>
          Application Description</th>
       
        </tr>
       </thead>
       <tbody class="rowlink" data-link="row">
       
       
        <% @portalmanager_appdefs.each do |portalmanager_appdef| %>
    <tr>
     <td><%= link_to portalmanager_appdef.application_title, portalmanager_appdef %></td>
     <td>   <%= portalmanager_appdef.description %></td>
      <td>
          <%= link_to(edit_portalmanager_appdef_path(portalmanager_appdef)) do  %><% end %></td>
         <td>
    
      
       <%= link_to(portalmanager_appdef_path(portalmanager_appdef), method: :delete, data: {confirm: "Are you sure?"}) do %><i class="glyphicon glyphicon glyphicon-trash icon-action" > </i><% end %>
       </td>
        

     </tr>
    <% end %>
       </tbody>
      </table>
     </div>
    </div>
    
 </div>
</div>
</div>
</div>
</div>


